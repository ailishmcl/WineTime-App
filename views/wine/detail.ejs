<div class="feedpage">
    <div class="index">
        <div class="topline"> 
            <h3><%= wine.name %></h3>
        </div>
        <div class="main">
            <div>Grape: <%= wine.grape %></div>
            <div>Region: <%= wine.region %></div>
            <div>Country: <%= wine.country %></div>
        </div>

    </div>
    <hr>
    

    
    
    
    <div> Reviews   </div>
        <% wine.review.forEach(function(review) { %>
    <div class="index"> 
           <%  review.user.forEach(function(user){ %>
            <div class="topline">
                <div> <a href="/user/detail?id=<%= user._id %>" class="navlink"> @<%= user.username %> </a> </div> 
                <%  })%>
                <div> Rating: <%= review.rating %> </div>
            </div>
           
           <div class="main">
                <div> <%= review.comment %> </div>
           </div>
        </div>    
        <% }) %>
    
    
</div>

